<h1>Articles#index</h1>
<p>Find me in app/views/articles/index.html.erb</p>
<%= link_to "new", new_article_path%>
<% @articles.each do |item|%>

<h2>
<%= link_to item.title, article_path(item)%>
<%= link_to "change", edit_article_path(item)%>
<%= link_to "Delete", article_path(item),
      method: :delete,
      data: { confirm: "Are you sure?" } %>
</h2>
<%end %>

